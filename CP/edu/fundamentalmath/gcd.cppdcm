#define ld long double
#define ll long long 
#define lli long long int 

int gcd (int a, int b) {
    if (b == 0)
        return a;
    else
        return gcd (b, a % b);
}//euclid talgorithm

int lcm (int a, int b) {
    return a / gcd(a, b) * b;
}